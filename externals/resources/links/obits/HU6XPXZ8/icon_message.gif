});
</script>

<script>
    $(function(){
	$.ajax({
	type: "GET",
	dataType: "jsonp",
	cache: true,
	jsonpCallback: "popular",
  	url: "http://gu-most-popular.appspot.com/api/most-viewed", 
    	success: function(data){
	   if(data.length >=5) {
		var header = "<h2>Popular on the Guardian now</h2>"
		$(".most-viewed").prepend(header)
	   	for(i=0; i<5; i++) {
	            var item = data[i]
	            var li = "<li><h4><a href="+item.webUrl+">"+item.fields.headline+"</a></h4><div class='trail-block'><a href="+item.webUrl+"><img src="+item.fields.thumbnail+"></a><p>"+item.fields.trailText+"</p></li>"
	            $(".most-viewed-items").append(li)
		}
           }
	}
    	});
   });
</script>


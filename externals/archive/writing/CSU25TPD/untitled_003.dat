                                uid   = $user.data('author-id');

                            var isMvnUser = false;

                            if (userId = uid.match(/^[^@]+/g)) {
                                if (/^[0-9]+$/.test(userId)) {
                                    isMvnUser = true;
                                }
                            }

                            if (fbTwitterTest = uid.match(/[^@]+$/)) {
                                if (/fb/.test(fbTwitterTest) || /tw/.test(fbTwitterTest)) {

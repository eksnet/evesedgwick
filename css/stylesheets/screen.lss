/* LESS variables */
@c_width:36px;
@c_margin:12px;

@black:#292627;
@dark:#70686B;
@light:#ccc;
@link1:#70686B; /* #249159 */
@link2:#F0AFE1; /*#17683C*/

@standard_left:0px 0px 0px 2px;
/* /LESS variables */

body {
	min-width:864px;
	margin:24px 12px 48px;
	padding:0px;
	color:#292627;
	font-family:arial, helvetica, sans-serif;
	font-size:12px;
	line-height:18px;
	font-weight:normal;
	text-align:left;
}

a, img, a img, iframe, form, fieldset, abbr, acronym, object, applet, table { 
	border:none; 
}

table { 
	border-collapse:collapse; 
	border-spacing:0; 
}

ul, ol, dir, menu { 
	list-style:none;
	padding:0px;
	margin:0px; 
}


img {
	border:none;
	margin:0px;
	padding:0px;
}

ul {
	list-style:none;
	margin:0px;
	padding:0px;
}

em {
	font-style:normal;
	font-weight:normal;
}

.cc_18 { width:((@c_width + @c_margin) * 18) - @c_margin; }
.cc_16 { width:((@c_width + @c_margin) * 16) - @c_margin; }
.cc_14 { width:((@c_width + @c_margin) * 14) - @c_margin; }
.cc_12 { width:((@c_width + @c_margin) * 12) - @c_margin; }
.cc_10 { width:((@c_width + @c_margin) * 10) - @c_margin; }
.cc_08 { width:((@c_width + @c_margin) * 8) - @c_margin; }
.cc_06 { width:((@c_width + @c_margin) * 6) - @c_margin; }
.cc_05 { width:((@c_width + @c_margin) * 5) - @c_margin;}
.cc_04 { width:((@c_width + @c_margin) * 4) - @c_margin; }
.cc_03 { width:((@c_width + @c_margin) * 3) - @c_margin; }
.cc_02 { width:((@c_width + @c_margin) * 2) - @c_margin; }
.cc_01 { width:((@c_width + @c_margin) * 1) - @c_margin; }

.bg1 {background-color:#8CB75D;}
.bg2 {background-color:#A9C38D;}
.bg3 {background-color:#BDDF98;}
.bg4 {background-color:#E3F9CA;}
.bg5 {background-color:#EFF9E4;}

#page_header {
	border-top:4px solid #292627;
}

#left_column {
	clear:left;
	float:left;
	margin:0px 12px 12px 0px;
	border-top:2px solid #292627;
}

#center_column {
	min-height:1020px;
	clear:none;
	float:left;
	margin:0px 12px 12px 0px;
	border-top:2px solid #292627;
}

#right_column {
	clear:none;
	float:left;
	margin:0px 0px 12px 0px;
	border-top:2px solid #292627;
}

#page_footer {
	border-top:2px solid #292627;
}

.side_module {
	border-bottom:1px solid #999;
}

.container {
	clear:both;
	float:left;
	margin:0px 0px 12px 0px;
}

.left {
	float:left;
	margin:0px 12px 0px 0px;
}
.right {
	float:right;
	margin:0px;
	text-align:right;
}


/* preset defs */
h1 {
	margin:0px;
	padding:17px 0px 46px 6px;
	font-size:22px;
	line-height:22px;
	text-transform:none;
	color:@black;
}
h2 { 
	margin:0px;
	padding:@standard_left;
	font-size:18px;
	line-height:32px;
	text-transform:none;
	color:@black;
}
h3 {
	margin:0px;
	padding:@standard_left;
	font-size:12px;
	line-height:28px;
	text-transform:uppercase;
	color:@black;
}
h4 {
	margin:0px; 
	padding:@standard_left;
	font-size:12px;
	line-height:28px;
	text-transform:none;
	color:#191a19;
}
h5 { 
	margin:0px;
	padding:0px 0px 0px 0px;
	font-size:10px;
	line-height:18px;
	font-weight:bold;
	text-transform:uppercase;
	color:#70686B;
}
h5.normal { 
	margin:0px;
	padding:0px 0px 0px 0px;
	font-size:10px;
	line-height:18px;
	font-weight:normal;
	text-transform:uppercase;
	color:#70686B;
}

h6 {
	margin:0px;
	padding:0px 0px 0px 0px;
	font-size:10px;
	line-height:18px;
	font-weight:normal;
	text-transform:none;
	color:#70686B;
}

p {
	margin:0px 0px 12px 0px;
}

a {
	color:@black;
	font-weight:bold;
	text-decoration:none;
	background-color:transparent;

}
a:link {
	color:@black;
	font-weight:bold;
	text-decoration:none;
	background-color:transparent;
}
a:visited {
	color:@black;
	font-weight:bold;
	text-decoration:none;
	background-color:transparent;
}
a:hover {
	color:@link1;
	background-color:transparent;
	font-weight:bold;
	text-decoration:underline;
}
a:active {
	color:@link2;
	font-weight:bold;
	text-decoration:underline;
	background-color:transparent;
}

a.block {
	color:@black;
	font-weight:bold;
	text-decoration:none;
	background-color:transparent;

}
a:link.block {
	color:@black;
	font-weight:bold;
	text-decoration:none;
	background-color:transparent;
}
a:visited.block {
	color:@black;
	font-weight:bold;
	text-decoration:none;
	background-color:transparent;
}
a:hover.block {
	color:white;
	background-color:@light;
	font-weight:bold;
	text-decoration:none;
	h3 {
		color:white;
		background-color:@light;
		font-weight:bold;
		text-decoration:none;
	}
}
a:active.block {
	color:@link2;
	font-weight:bold;
	text-decoration:underline;
	background-color:transparent;
}


.image_container_left {
	float:left;
	width:132px;
	margin:5px 12px 0px 2px;
}
.image_container_right {
	float:right;
	width:132px;
	margin:5px 0px 0px 12px;
}
.image_container_center {
	width:276px;
	text-align:center;
	margin:0px auto 12px;
	overflow:hidden;
}

.caption {
	font-size:11px;
	text-align:left;
	margin:0px;
	padding:0px 0px 0px 2px;
}
.caption em {
	font-weight:bold;
	text-decoration:none;
}

#navigation {
	padding:6px 0px 12px;
	.rule {
		height:8px;
		margin-bottom:10px;
		border-bottom:1px solid #ccc;
	}
	a {
		color:#292627;
		display:block;
		padding:2px 0px 2px 3px;
		font-size:12px;
		line-height:16px;
	}
	a:link {
		color:#70686b;
		font-weight:normal;
		text-decoration:none;
		background-color:transparent;
	}
	a:visited {
		color:#70686b;
		font-weight:normal;
		text-decoration:none;
		background-color:transparent;
	}
	a:hover {
		color:#249159;
		font-weight:normal;
		text-decoration:underline;
		background-color:transparent;
	}
	a:active {
		color:#17683c;
		font-weight:normal;
		text-decoration:underline;
		background-color:transparent;
	}
	a.menu {
		font-weight:bold;
		text-transform:uppercase;
	}
	a:link.menu { color:#292627; }
	a:visited.menu { color:#292627; }
	a:hover.menu {
		color:#249159;
		text-decoration:none;
		background-color:transparent;
	}
	a:active.menu {
		color:#17683c;
		text-decoration:none;
		background-color:transparent;
	}
	a.nonlinking {
		font-weight:bold;
		text-transform:uppercase;
	}
	a:link.nonlinking { color:#292627; }
	a:visited.nonlinking { color:#292627; }
	a:active.nonlinking {
		color:#292627;
		text-decoration:none;
		background-color:transparent;
	}
	a:hover.nonlinking {
		color:#292627;
		text-decoration:none;
		background-color:transparent;
	}
	a:link.current {
		color:#292627;
		font-weight:bold;
		text-decoration:underline;
		background-color:transparent;
	}
	a:visited.current {
		color:#292627;
		font-weight:normal;
		text-decoration:none;
		background-color:transparent;
	}
	a.sub {
		color:#292627;
		display:block;
		margin-left:10px;
		font-size:11px;
		line-height:16px;
	}
	li.leaf { margin-left:8px; }
	ul.sub_menu a:link.current {
		color:#292627;
		font-weight:normal;
		text-decoration:underline;
		background-color:transparent;
	}
	ul.sub_menu a:visited.current {
		color:#292627;
		font-weight:normal;
		text-decoration:none;
		background-color:transparent;
	}
}

.post {
	margin:0px 0px 0px 0px;
	border-top:1px solid @black;

	.header {
		border-bottom:1px solid #999;
	}
	.body {
		.left_column p.post_date {
			color:#70686B;
			font-size:11px;
			line-height:16px;
			font-weight:bold;
			text-transform:uppercase;
		}
		.content {
		}
	}
}

.album {
	clear:none;
	margin:0px 12px 0px 0px;
	border-top:1px solid @black;
	.header {
		border-bottom:1px solid #999;
		margin:0px 0px 8px 0px;
	}
	.footer {
		border-bottom:1px solid @black;
	}
	.body {
		margin:0px 0px 10px 0px;
		.thumb {
			height:116px;
			width:84px;
			background-color:@black;
		}
	}
}

.document {
	margin:0px 0px 0px 0px;
	border-top:1px solid @black;
	.header {
		border-bottom:1px solid #999;
	}
	.footer {
	}
	.body {
		p {
			padding:@standard_left;
		}
		p.caption {
			padding:0px 0px 0px 2px;
		}
		.toc {
			border-top:1px solid #999;
			border-bottom:1px solid #999;
			h4 {
				color:#999;
				border-bottom:1px solid #999;
			}
			table {
				padding:6px 0px
			}
			ul.left_list {
			}
			.toc_left {
				float:left;
				text-align:left;
			}
			.gutter {
				width:12px;
			}
			a.toc_item {
				border:none;
				text-align:right;
				display:block;
				padding:0px 2px 0px 2px;
			}
			.content {
			}
		}
	}
}

.footer .column {
	padding-top:6px;
}



/* Universal Overides */

.nm { margin:0px; }
.cn { clear:none; }

.text_r { text-align:right; }
.text_l { text-align:left; }
.text_c { text-align:center; }

.rule_t { border-top:1px solid #999; }
.rule_b { border-bottom:1px solid @black; }

.bg1, .bg2, .bg3, .bg4, .bg5 {background-color:transparent;}